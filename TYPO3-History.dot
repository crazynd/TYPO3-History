    "TYPO3 4.2.4–4.2.10" -> "TYPO3 4.3.0";
    "TYPO3 4.3.1–4.3.10" -> "TYPO3 4.4.0–4.4.6";
    "TYPO3 4.4.7" -> "TYPO3 4.5.0";
}
