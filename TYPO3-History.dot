digraph t3history {
    label="History of TYPO3";
    labelloc=t;
    rankdir=LR;
    ranksep=equally;
    size="11.69,8.27";
    fontname="Helvetica";

    { /* Timeline… */
        node [shape=plaintext];
       /* 1997 -> 1998 -> 1999 -> 2000 -> 2001 -> */ 2002 ->
        2003 -> 2004 -> 2005 -> 2006 -> 2007 -> 2008 ->
        2009 -> 2010 -> 2011;
    }

    node [shape=box];
    { rank = same; 2002;
      "TYPO3 3.3.0";
    }
    { rank = same; 2003;
      "TYPO3 3.5.0";
    }
    { rank = same; 2004;
      "TYPO3 3.6.0–3.6.2";
      "TYPO3 3.7.0" 
    }
    { rank = same; 2005;
      "TYPO3 3.7.1";
      "TYPO3 3.8.0–3.8.1";
    }
    { rank = same; 2006;
      "TYPO3 4.0.0–4.0.3";
    }
    { rank = same; 2007;
      "TYPO3 4.0.4–4.0.8";
      "TYPO3 4.1.0–4.1.5";
    }
    { rank = same; 2008;
      "TYPO3 4.0.9";
      "TYPO3 4.1.6–4.1.7";
      "TYPO3 4.2.0–4.2.3";
      "FLOW3 1.0.0a1"
    }
    { rank = same; 2009;
      "TYPO3 4.0.10–4.0.13 ☠";
      "TYPO3 4.1.8–4.1.13";
      "TYPO3 4.2.4–4.2.10";
      "TYPO3 4.3.0";
      "FLOW3 1.0.0a2–1.0.0a6"
    }
    { rank = same; 2010;
      "TYPO3 4.1.14–4.1.15 ☠";
      "TYPO3 4.2.11–4.2.17";
      "TYPO3 4.3.1–4.3.10";
      "TYPO3 4.4.0–4.4.6";
      "FLOW3 1.0.0a7–1.0.0a14";
    }
    { rank = same; 2011;
      "TYPO3 4.4.7";
      "TYPO3 4.5.0";
    }

    /* 3.7-line: */
    "TYPO3 3.7.0" -> "TYPO3 3.7.1";
    /* 4.0-line: */
    "TYPO3 4.0.0–4.0.3" -> "TYPO3 4.0.4–4.0.8" -> "TYPO3 4.0.9" -> "TYPO3 4.0.10–4.0.13 ☠";
